#include <stdio.h>

// Функція для знаходження НСД двох чисел
int gcd(int a, int b) {
    if (b == 0)
        return a;
    return gcd(b, a % b);
}

// Функція для знаходження НСК двох чисел
int lcm(int a, int b) {
    return (a * b) / gcd(a, b);
}

int main() {
    int n; // Кількість чисел
    scanf("%d", &n);

    int nums[n]; // Ряд натуральних чисел

    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }

    int result = nums[0]; // Ініціалізуємо результат першим числом

    // Знаходимо НСК для всіх чисел у ряду
    for (int i = 1; i < n; i++) {
        result = lcm(result, nums[i]);
    }

    printf("%d\n", result); // Виводимо результат - НСК

    return 0;
}
